{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@components": [
        "src/components/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@configs/*": [
        "src/configs/*"
      ],
      "@aki_types/*": [
        "src/types/*"
      ]
    }
  },
  "include": [
    "vite.config.ts"
  ]
}